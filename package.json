{
  "name": "webflow-devstarter-tmbu",
  "version": "1.0.0",
  "description": "A minimal TypeScript starter for Webflow custom code integration",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "pnpm format && pnpm lint && pnpm check",
    "mcp": "npx webflow-mcp-server"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.40.0",
    "gsap": "^3.12.5",
    "webflow-api": "^3.1.1",
    "webflow-mcp-server": "^0.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@finsweet/tsconfig": "^1.3.3",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.1.5"
  }
}